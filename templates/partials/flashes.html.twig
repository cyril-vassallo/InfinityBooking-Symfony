{% for label, messages in app.flashes %}
    <div class="container">
        <div>
        {% for message in messages %}
            <div class="alert alert-dismissible alert-{{label}}">
                <button type="button" class="close" data-dismiss="alert">&times;</button>
                {{message|raw}}
            </div>
        {% endfor %}
        </div>
    </div>
{% endfor %}


